{
        "manifest_version":"1",
        "type":"WebApp",
        "meta":{
                "name":"Event listener",
                "version":"1.0.1",
                "minimum-overwolf-version":"0.87.0",
                "author":"Peter Sobkowski",
                "icon":"IconMouseOver.png",
                "icon_gray":"IconMouseNormal.png",
                "description":"Event Listener"
        },
        "permissions": ["GameInfo"],
 "data": {
        "start_window": "MainWindow",
        "windows": {
            "MainWindow": {
                "file": "Files/index.html",
                "transparent": true,
                "resizable": true,
                "size": {"width": 400, "height": 300},
                "min_size": {"width": 300, "height": 300},
                "max_size": {"width": 600, "height": 500}
            }
        }
    }
}